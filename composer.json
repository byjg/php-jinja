{
  "name": "byjg/jinja-php",
  "description": "Lightweight PHP implementation of the Jinja2 template engine originally developed for Python",
  "type": "library",
  "require": {
    "php": ">=8.3 <8.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.5",
    "vimeo/psalm": "^5.9|^6.13"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ByJG\\JinjaPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --threads=1"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
